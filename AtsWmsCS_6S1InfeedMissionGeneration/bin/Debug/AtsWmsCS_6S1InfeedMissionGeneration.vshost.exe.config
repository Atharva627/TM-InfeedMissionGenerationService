<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
      <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    </configSections>
    <connectionStrings>
        <add name="AtsWmsCS_6S1InfeedMissionGeneration.Properties.Settings.ats_tata_metallics_dbConnectionString"
            connectionString="Data Source=localhost;Initial Catalog=ats_tata_metallics_db;User ID=ats-india;Password=Ats123*"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <log4net>

    <!--1. service class-->
    <appender name="AtsWmsCS_6S1InfeedMissionGenerationDebug" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="D:\\ATS_WMS_LOGS_ROOT\\AtsWmsCS_6S1InfeedMissionGeneration\\AtsWmsCS_6S1InfeedMissionGeneration\\Debug\\AtsWmsCS_6S1InfeedMissionGenerationDebug.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="dd-MMMM-yyyy"/>
      <maxSizeRollBackups value="100"/>
      <maxDateRollBackups value="3"/>
      <maximumFileSize value="50MB"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG"/>
        <levelMax value="DEBUG"/>
      </filter>
      <filter class="log4net.Filter.DenyAllFilter"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>
    <appender name="AtsWmsCS_6S1InfeedMissionGenerationError" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="D:\\ATS_WMS_LOGS_ROOT\\AtsWmsCS_6S1InfeedMissionGeneration\\AtsWmsCS_6S1InfeedMissionGeneration\\Error\\AtsWmsCS_6S1InfeedMissionGenerationError.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="dd-MMMM-yyyy"/>
      <maxSizeRollBackups value="100"/>
      <maxDateRollBackups value="3"/>
      <maximumFileSize value="50MB"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR"/>
        <levelMax value="ERROR"/>
      </filter>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>

    <logger name="AtsWmsCS_6S1InfeedMissionGeneration">
      <appender-ref ref="AtsWmsCS_6S1InfeedMissionGenerationError"/>
      <appender-ref ref="AtsWmsCS_6S1InfeedMissionGenerationDebug"/>
    </logger>



    <!--2. Business class-->
    <appender name="AtsWmsCS_6S1InfeedMissionGenerationDetailsDebug" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="D:\\ATS_WMS_LOGS_ROOT\\AtsWmsCS_6S1InfeedMissionGeneration\\AtsWmsCS_6S1InfeedMissionGenerationDetails\\Debug\\AtsWmsCS_6S1InfeedMissionGenerationDetailsDebug.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="dd-MMMM-yyyy"/>
      <maxSizeRollBackups value="100"/>
      <maxDateRollBackups value="3"/>
      <maximumFileSize value="50MB"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG"/>
        <levelMax value="DEBUG"/>
      </filter>
      <filter class="log4net.Filter.DenyAllFilter"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>
    <appender name="AtsWmsCS_6S1InfeedMissionGenerationDetailsError" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="D:\\ATS_WMS_LOGS_ROOT\\AtsWmsCS_6S1InfeedMissionGeneration\\AtsWmsCS_6S1InfeedMissionGenerationDetails\\Error\\AtsWmsCS_6S1InfeedMissionGenerationDetailsError.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="dd-MMMM-yyyy"/>
      <maxSizeRollBackups value="100"/>
      <maxDateRollBackups value="3"/>
      <maximumFileSize value="50MB"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR"/>
        <levelMax value="ERROR"/>
      </filter>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>

    <logger name="AtsWmsCS_6S1InfeedMissionGenerationDetails">
      <appender-ref ref="AtsWmsCS_6S1InfeedMissionGenerationDetailsError"/>
      <appender-ref ref="AtsWmsCS_6S1InfeedMissionGenerationDetailsDebug"/>
    </logger>


    <root>
      <level value="ALL"/>
      <appender-ref ref="FileAppender"/>
      <appender-ref ref="ConsoleAppender"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
  </log4net>
</configuration>